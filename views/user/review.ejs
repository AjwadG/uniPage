<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>FeadBack</title>

    <link rel="icon" type="image/x-icon"  href="content/images/345832210_1468513350593187_453318926271352151_n-removebg.png">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="css/user.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-KK94CHFLLe+nY2dmCWGMq91rCGa5gtU4mk92HdvYe+M/SXH301p5ILy+dN9+nJOZ" crossorigin="anonymous">
</head>
<body>

    <div class="top-bar">
        <div id="header">
            <div class="logo-cont">
                <a href="/"><img src="content/images/345832210_1468513350593187_453318926271352151_n-removebg.png" class="logo" alt="cetz"></a>
                <p class="college-name" >كلية التقنية الهندسية زوارة</p>
            </div>
            <input type="checkbox" id="check">
              <label for="check" class="checkbtn">
                  <i class="fa fa-bars"></i>
            </label>
            <div class="navItems">
                <a href="/students" style="text-decoration: double; color: white; font-size: 24px; margin-right: 15px;">الطلبة</a>
                <a href="/workshops" style="text-decoration: double; color: white; font-size: 24px; margin-right: 15px;">اضافة ورشة العمل</a>
                <a href="/adduser" style="text-decoration: double; color: white; font-size: 24px; margin-right: 15px;">اضافة مشرف</a>
                <a href="/logout" class="btn btn-danger">تسجيل الخروج</a>
            </div>
        </div>
   </div>

    <div class="testimonial">
       <div class="container mt-5">
                <div class="titlepage mt-5">
                   <h2>FeadBack</h2>
                </div>
                <% tables.forEach(table => { %>
                <div class="titlepage mt-5">
                    <h2><%= table[0] %></h2>
                 </div>
                <div class="card-list-container">
                    <div class="card-list">
                        <% table[1].forEach(row => { %>

                        <div class="card">
                            <div class="card-content">
                                <h2></h2>
                                <% for (var cell in row) {
                                    if (Object.prototype.hasOwnProperty.call(row, cell)) { %>
                                        <h4><%= cell %> : <%- row[cell] %></h4>
                               <%   }
                                  } %>
                                    <button class="sign-btn btn btn-danger" id="sign-btn">حذف</button>
                                    <div class="model" >
                                        <div class="model-header" dir="rtl">
                                           <div class="model-title"> <h3>تنبيه</h3> </div>
                                           <button class="close-btn" id="close-btn">&times;</button>
                                        </div>
                                        <div class="model-body">
                                         <h3>هل انت متاكد من حدف <%= row.id %></h3>
                                        </div>
                                        <div style="display: flex; align-items: center; justify-content: center;">
                                            <form action="/review" method="post">
                                                <input type="text" style="display: none;" name="id" value="<%= row.id %>">
                                                <input type="text" style="display: none;" name="table" value="<%= table[0] %>">
                                                <button class="btn btn-danger mb-3 " style="text-decoration: none;">حذف</button>
                                            </form>
                                        </div>
                                    </div>
                            </div>
                        </div>
                       
                        <% }); %>
                    </div>
                </div>
                <% }); %>
            </div>
        </div>

        <script src="js/index.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ENjdO4Dr2bkBIFxQpeoTz1HIcje39Wm4jDKdf19U8gI4ddQ3GYNS7NTKfAdVQSZe" crossorigin="anonymous"></script>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
</body>
</html>
 